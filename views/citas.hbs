<table border="solid">
<h2>LISTADO DE CITAS</h2>
<thead>
<tr>
<th>Paciente</th>
<th>Medico</th>
<th>Fecha</th>
<th>Opciones</th>
</tr>
</thead>
<tbody>
{{#each citas}}
<tr>
<td>{{nombre}}</td>
<td>{{nombres}}</td>
<td>{{formatoFecha fecha}}</td>
<td><a class="eliminar" href="citas/eliminar/{{id}}">Cancelar</a></td>
</tr>
{{/each}}
</tbody>
</table>
<a href="/citas/agregar">Agregar cita medica</a>